        </activity>

        <activity
            android:name="rocks.che.elections.minigames.HammerGameActivity"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name="rocks.che.elections.minigames.LadderGameActivity"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name="rocks.che.elections.minigames.SnakeGameActivity"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name="rocks.che.elections.minigames.RunnerGameActivity"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name="rocks.che.elections.SpendMoneyActivity"
            android:screenOrientation="portrait">

        </activity>
    </application>

</manifest>